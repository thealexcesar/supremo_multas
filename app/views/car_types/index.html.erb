<div>
  <h1 class="h3 left"> <%= t('activerecord.models.car_type') %> </h1>

  <div class="right mt-10">
    <%= link_to new_car_type_path do %>
      <button type="button" class="btn btn-default supremo-btn-gray text-light"><%= new_model %></button>
    <% end %>
  </div>
</div>
<table class="table">
  <thead>
    <tr class="supremo-table-top-color">
      <th><%= t("general.name") %></th>
      <th colspan="3"><%= t("general.actions") %></th>
    </tr>
  </thead>

  <tbody>
    <% @car_types.each do |car_type| %>
      <tr>
        <td><%= car_type.name %></td>
        <td><%= link_to t('general.show'), car_type %></td>
        <td><%= link_to t('general.edit'), edit_car_type_path(car_type) %></td>
        <td><%= link_to t('general.delete'), car_type, method: :delete, data: {confirm: t("general.messages.confirm")} %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<div class="right">
  <div class="supremo_pagination">
    <%= will_paginate @users, previous_label: fa_icon('angle-left'), next_label: fa_icon('angle-right') %>
  </div>
</div>