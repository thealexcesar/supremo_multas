<%= form_with(model: fine, local: true) do |form| %>
  <% if fine.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(fine.errors.count, "error") %> prohibited this fine from being saved:</h2>

      <ul>
      <% fine.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :user_id %>
    <%= form.text_field :user_id %>
  </div>

  <div class="field">
    <%= form.label :fine_date %>
    <%= form.datetime_select :fine_date %>
  </div>

  <div class="field">
    <%= form.label :fine_status %>
    <%= form.number_field :fine_status %>
  </div>

  <div class="field">
    <%= form.label :fine_number %>
    <%= form.text_field :fine_number %>
  </div>

  <div class="field">
    <%= form.label :branch_id %>
    <%= form.number_field :branch_id %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
