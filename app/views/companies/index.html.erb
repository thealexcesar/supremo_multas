<div>
  <h1 class="h3 left"> <%= t("activerecord.models.company").pluralize %> </h1>

  <div class="right mt-10">
    <%= link_to new_company_path do %>
      <button type="button" class="btn btn-default supremo-btn-gray text-light"><%= new_model_f %></button>
    <% end %>
  </div>
</div>
<table class="table">
  <thead>
    <tr class="supremo-table-top-color">
      <th><%= t("activerecord.attributes.company.name") %></th>
      <th><%= t("activerecord.attributes.company.company_type") %></th>
      <th><%= t("activerecord.attributes.company.cnpj") %></th>
      <th><%= t("activerecord.attributes.company.phone") %></th>
      <th><%= t("activerecord.attributes.company.address") %></th>
      <th><%= t("activerecord.attributes.company.number") %></th>
      <th><%= t("activerecord.attributes.company.zipcode") %></th>
      <th><%= t("activerecord.attributes.company.district") %></th>
      <th><%= t("activerecord.attributes.company.city_id") %></th>
      <th><%= t("activerecord.models.state") %></th>
      <th colspan="3" class="text-center"><%= t("general.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @companies.each do |company| %>
      <tr>
        <td><%= company.name %></td>
        <td><%= enum_to_name(company, :company_type) %></td>
        <td><%= company.cnpj %></td>
        <td><%= company.phone %></td>
        <td><%= company.address %></td>
        <td><%= company.number %></td>
        <td><%= company.zipcode %></td>
        <td><%= company.district %></td>
        <td><%= company.city&.name %></td>
        <td><%= company.city&.state&.name %></td>
        <td><%= link_to t("general.show"), company %></td>
        <td><%= link_to t("general.edit"), edit_company_path(company) %></td>
        <td><%= link_to t("general.delete"), company, method: :delete, data: { confirm: t("general.messages.confirm") } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<div class="right">
  <div class="supremo_pagination">
    <%= will_paginate @users, previous_label: fa_icon('angle-left'), next_label: fa_icon('angle-right') %>
  </div>
</div>
